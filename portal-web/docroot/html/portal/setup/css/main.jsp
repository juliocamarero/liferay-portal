body{
	font-size: 1.1em;
	background-color: #ECF1EF;
}

#setup-summary {
	
}

#setup-summary ol li.header {
	list-style-type: none;
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 10px;
	margin: 0;
	color: #ECF1EF;
	background: #363636;
	font-weight: bold;
}

#setup-wrapper {
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background: #ECF1EF;
	color: #363636;
	min-height: 200px;
	margin-left: 20%;
	margin-right: 20%;
}	

#setup-wrapper div.wrapper {
	padding: 10px;
	border: 1px solid #363636;
}

#setup-wrapper div.wrapper td {
	padding-top: 5px;
	padding-bottom: 5px;
}

#setup-wrapper h2.fieldset-header {
	list-style-type: none;
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 10px;
	margin: 0;
	font-weight: bold;
	color: #ECF1EF;
	background: #363636;
	font-size: 1em;
}

#setup-wrapper .first {
	-webkit-border-top-left-radius: 10px;
	-webkit-border-top-right-radius: 10px;
	-moz-border-radius-topleft: 10px;
	-moz-border-radius-topright: 10px;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}

#setup-wrapper .last {
	-webkit-border-bottom-right-radius: 10px;
	-webkit-border-bottom-left-radius: 10px;
	-moz-border-radius-bottomright: 10px;
	-moz-border-radius-bottomleft: 10px;
	border-bottom-right-radius: 10px;
	border-bottom-left-radius: 10px;
}

#setup-wrapper .middle {
	border-top: 1px dotted #ffffff;
	border-bottom: 1px dotted #ffffff;
}

#setup-wrapper .setup-settings-done {
	background: transparent url(/html/portal/setup/images/success.png) 0 110% no-repeat;
	width: 24px;
}

#setup-wrapper .tooltip {
	font-size: 0.8em;
	font-style: italic;
}

#wrapper-button {
	padding-top: 15px;
}

#wrapper-image {
	padding: 2px;
	border: 1px solid #363636;
}

.bigger {
	font-size: larger;
}

.msg-success {
	background: #F4FDEF url(/html/portal/setup/images/success.png) no-repeat 6px 20%;
	border: 1px solid #ACDFA7;
	color: #384F34;
	display: block;
	font-weight: bold;
	margin: 2px auto 14px;
	padding: 6px 6px 6px 30px;
	text-align: left;
}

.msg-alert {
	background: #FFC url(/html/portal/setup/images/alert.png) no-repeat 6px 20%;
	border: 1px solid #FC0;
	display: block;
	font-weight: bold;
	margin: 2px auto 14px;
	padding: 6px 6px 6px 30px;
	text-align: left;
}