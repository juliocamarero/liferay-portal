Bundle-Name: Liferay RSS Taglib
Bundle-SymbolicName: com.liferay.rss.taglib
Bundle-Version: 1.0.0
Export-Package:\
	com.liferay.rss.taglib,\
	com.liferay.rss.taglib.servlet.taglib
Include-Resource:\
	classes,\
	META-INF/resources=src/main/resources/META-INF/resources
Provide-Capability:\
	osgi.extender;osgi.extender="jsp.taglib";uri="http://liferay.com/tld/rss";version:Version="1.0.0"
Require-SchemaVersion: 1.0.0
Web-ContextPath: /rss-taglib
